{
"model":{
	"id":"posterior",
	"type" : "compound",
	"distributions":[
		{
			"id":"treelikelihood",
			"type": "treelikelihood",
			"sitepattern":{
				"id":"patterns",
				"type":"sitepattern",		
				"datatype":"nucleotide",
				"alignment":{
					"id": "seqs",
					"type": "alignment",
					"file":"DATA_TEMPLATE",
					"datatype":"nucleotide"
				}
			},
			"sitemodel":{
				"id": "sitemodel",
				"type": "sitemodel",
				"substitutionmodel":{
					"id":"sm",
					"type":"substitutionmodel",
					"model":"jc69",
					"datatype":"nucleotide",
					"frequencies":{
						"id":"freqs",
						"type":"Simplex",
						"values":[0.25,0.25,0.25,0.25] 
					}
				}
			},
			"tree":{
				"id":"tree",
				"type":"tree",
				"parameters":"tree.distances",
				"newick":"TREE_TEMPLATE"
			}
		},
		{
			"id":"priorbl",
			"type": "distribution", 
			"distribution": "exponential",
			"parameters":{
				"lambda":{"id":"lambdabl", "type":"parameter", "value":10, "lower":0}
			},
			"tree":"&tree"
		}
	]
},
"physher":[
	{
		"id": "bridge",
		"type": "bridgesampling",
		"file":"LOG_TEMPLATE",
		"burnin": 100,
		"model": "&posterior",
		"x": "%tree.distances"
	}
]
}
