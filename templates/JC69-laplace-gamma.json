{
"model":{
	"id":"posterior",
	"type" : "compound",
	"distributions":[
		{
			"id":"treelikelihood",
			"type": "treelikelihood",
			"sitepattern":{
				"id":"patterns",
				"type":"sitepattern",		
				"datatype":"nucleotide",
				"alignment":{
					"id": "seqs",
					"type": "alignment",
					"file":"DATA_TEMPLATE",
					"datatype":"nucleotide"
				}
			},
			"sitemodel":{
				"id": "sitemodel",
				"type": "sitemodel",
				"substitutionmodel":{
					"id":"sm",
					"type":"substitutionmodel",
					"model":"jc69",
					"datatype":"nucleotide",
					"frequencies":{
						"id":"freqs",
						"type":"Simplex",
						"values":[0.25,0.25,0.25,0.25] 
					}
				}
			},
			"tree":{
				"id":"tree",
				"type":"tree",
				"parameters":"tree.distances",
				"_file":"temp.tree",
				"newick":"TREE_TEMPLATE"
			}
		},
		{
			"id":"priorbl",
			"type": "distribution", 
			"distribution": "exponential",
			"parameters":{
				"lambda":{"id":"lambdabl", "type":"parameter", "value":10, "lower":0}
			},
			"tree":"&tree"
		}
	]
},
"physher":[
	{
		"id":"metaopt",
		"type":"optimizer",
		"algorithm":"meta",
		"precision":0.0000001,
		"max": 10000,
		"min": 1,
		"model":"&posterior",
		"list":[
			{				
				"id":"optbl",
				"type":"optimizer",
				"algorithm":"serial",
				"model":"&posterior",
				"treelikelihood": "&treelikelihood",
				"precision":0.00000001
			}
		]
	},
	{
		"id": "log",
		"type": "logger",
		"models": ["&posterior","&treelikelihood"]
	},
	{
		"id":"laplaceG",
		"type":"laplace",
		"model": "&posterior",
		"distribution": "gamma",
		"parameters": ["%tree.distances"]
	}
]
}
