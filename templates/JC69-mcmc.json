{
"model":{
	"id":"posterior",
	"type" : "compound",
	"distributions":[
		{
			"id":"treelikelihood",
			"type": "treelikelihood",
			"sitepattern":{
				"id":"patterns",
				"type":"sitepattern",		
				"datatype":"nucleotide",
				"alignment":{
					"id": "seqs",
					"type": "alignment",
					"file":"DATA_TEMPLATE",
					"datatype":"nucleotide"
				}
			},
			"sitemodel":{
				"id": "sitemodel",
				"type": "sitemodel",
				"substitutionmodel":{
					"id":"sm",
					"type":"substitutionmodel",
					"model":"jc69",
					"datatype":"nucleotide",
					"frequencies":{
						"id":"freqs",
						"type":"Simplex",
						"values":[0.25,0.25,0.25,0.25] 
					}
				}
			},
			"tree":{
				"id":"tree",
				"type":"tree",
				"parameters":"tree.distances",
				"newick":"TREE_TEMPLATE"
			}
		},
		{
			"id":"priorbl",
			"type": "distribution", 
			"distribution": "exponential",
			"parameters":{
				"lambda":{"id":"lambdabl", "type":"parameter", "value":10, "lower":0}
			},
			"tree":"&tree"
		}
	]
},
"physher":[
	{
		"id":"metaopt",
		"type":"optimizer",
		"algorithm":"meta",
		"precision":0.001,
		"max": 10000,
		"min": 1,
		"model":"&posterior",
		"list":[
			{				
				"id":"optbl",
				"type":"optimizer",
				"algorithm":"serial",
				"model":"&posterior",
				"treelikelihood": "&treelikelihood"
			}
		]
	},
	{
		"id": "log",
		"type": "logger",
		"models": ["&posterior","&treelikelihood"]
	},
	{
		"id":"mcmc",
		"type":"mcmc",
		"model":"&posterior",
		"length": 1000000,
		"log":[
			{
				"file": "LOG_TEMPLATE",
				"every": 1000,
				"models":["&posterior", "&treelikelihood"],
				"x": "%tree.distances"
			},
			{
				"cpo": true,
				"file": "pll-LOG_TEMPLATE",
				"every": 1000,
				"models":["&treelikelihood"]
			}
		],
		"operators":[
			{
				"id": "scaler",
				"type":"operator",
				"algorithm": "scaler",
				"x": "%tree.distances",
				"weight": 1
			},
			{
				"id": "slider",
				"type":"operator",
				"algorithm": "slider",
				"x": "%tree.distances",
				"weight": 1
			}
		]
		
	}
]
}
